{
	"AWSEBDockerrunVersion":2,
	"containerDefinitions": [
		{
		    "name": "client",
			"image": "adityayadav/multi-client",
			"hostname": "client",
			"essential": false
		},
		{
			"name": "worker",
			"image": "adityayadav/multi-worker",
			"hostname": "worker",
			"essential": false
		},
		{
			"name": "server",
			"image": "adityayadav/multi-server"
			"hostname": "server"
			"essential": false
		},
		{
			"name": "nginx",
			"image": "adityayadav/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMapping": [
				{
				"hostPort": 80,
				"containerPort": 80
				}
			],
			"links": ["client", "server"]
		}
	]
}